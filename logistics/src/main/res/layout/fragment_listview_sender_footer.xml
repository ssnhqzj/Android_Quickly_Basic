<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dgfc_self_address"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:paddingRight="20dp"
    android:paddingLeft="20dp"
    android:paddingTop="5dp"
    android:paddingBottom="5dp">

    <com.qzj.logistics.ui.DrawableCenterTextView
        android:id="@+id/dgfc_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:layout_below="@+id/dgfc_self_address_detail"
        android:background="@drawable/common_btn_main_selector_r"
        android:drawableLeft="@mipmap/add"
        android:drawablePadding="5dp"
        android:clickable="true"
        android:textColor="@color/common_white"
        android:text="添加"/>

</RelativeLayout>
